{
  "name": "conway-game-of-life--react-vs-preact-vs-solid",
  "version": "0.0.0",
  "license": "WTFPL",
  "private": true,
  "scripts": {
    "build": "npm-run-all -p build:*",
    "build:react": "cd react && npm run build",
    "build:preact": "cd preact && npm run build",
    "build:solid": "cd solid && npm run build",
    "build:vue": "cd vue && npm run build",
    "watch": "npm-run-all -p watch:*",
    "watch:react": "cd react && npm run watch",
    "watch:preact": "cd preact && npm run watch",
    "watch:solid": "cd solid && npm run watch",
    "start": "npm run watch"
  },
  "prettier": {
    "printWidth": 140,
    "endOfLine": "lf",
    "singleQuote": true,
    "trailingComma": "all",
    "semi": false,
    "arrowParens": "avoid"
  },
  "devDependencies": {
    "npm-run-all2": "^6.0.5"
  }
}